{
  "manifest_version": 3,
  "name": "Devtools for Frontends",
  "version": "1.0",
  "description": "Extension for frontend developers, make more pretty the frontend",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["index.js"],
      "css": ["index.css"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}